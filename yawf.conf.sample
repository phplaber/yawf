# [[请求]]
[request]

# 代理
proxy : 

# 请求 scheme
scheme : https

# 请求超时时间（秒）
timeout : 30

# 自定义 User-Agent
user_agent : 

# [[探针]]
[probe]

# 默认探针
default : sqli, xss

# 自定义探针
customize : xss

# [[CEYE.IO]]
[ceye]

# Identifier
id : 

# API Token
token : 

# [[Azure openai]]
[azure]

# 是否使用 azure openai 服务
enabled : off

# azure 部署名称
deployment_name : 

# Endpoint
api_base : 

api_version : 2023-05-15

# 密钥
api_key : 

# [[杂项]]
[misc]

# 测试目标运行平台 [linux|windows]
platform : linux

# 是否检测 waf [on|off]
enable_waf_detecter : on
