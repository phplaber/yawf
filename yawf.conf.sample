# [[请求]]
[request]

# 代理
proxy : 

# 请求 scheme
scheme : https

# 请求超时时间（秒）
timeout : 30

# 自定义 User-Agent
user_agent : 

# [[探针]]
[probe]

# 默认探针
default : sqli, xss

# 自定义探针
customize : xss

# [[CEYE.IO]]
[ceye]

# Identifier
id : 

# API Token
token :  

# [[大模型]]
[llm]

# 使用状态 [disable|enable]
status : disable

# 模型
model : 

# 大模型基础 URL
base_url : 

# 大模型 API 调用凭证
api_key : 

# [[杂项]]
[misc]

# 测试目标运行平台 [linux|windows]
platform : linux
